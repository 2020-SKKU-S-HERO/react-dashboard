{
  "name": "react-dashboard",
  "version": "0.1.1",
  "private": true,
  "author": "701b",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^24.9.1",
    "@types/morgan": "^1.9.1",
    "@types/node": "^12.12.59",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.5",
    "morgan": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "clean": "rm -rf build",
    "start": "cross-env NODE_ENV=production node ./build/main.js",
    "dev": "concurrently \"npm run dev_server\" \"npm run dev_react\"",
    "build": "npm run build_react && npm run build_server",
    "dev_react": "react-scripts start",
    "dev_server": "cross-env NODE_ENV=development nodemon server/js/main.js",
    "build_react": "react-scripts build",
    "build_server": "babel server/js --out-dir build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {}
}
